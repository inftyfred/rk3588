{
    "configurations": [
        {
            "name": "Linux", // 配置名称，可以根据平台命名
            "includePath": [
                "${workspaceFolder}/**", // 默认包含工作目录下的所有文件
                "/usr/include", // 系统头文件路径
                "/usr/local/include", // 本地安装的头文件路径
                "/path/to/your/custom/include" // 自定义头文件路径
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc", // 编译器路径
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}